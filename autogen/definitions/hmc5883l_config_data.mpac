  uint8 fs_mag # default: 1
  uint32 n_calib_samples
  float dev2body_angle_1_deg >> mlt(@,deg2rad) >> dev2body_angle_1
  float dev2body_angle_2_deg >> mlt(@,deg2rad) >> dev2body_angle_2
  float dev2body_angle_3_deg >> mlt(@,deg2rad) >> dev2body_angle_3
